<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/mainPageGuidLine"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:orientation="vertical"
            app:layout_constraintGuide_end="349dp"
            app:layout_constraintStart_toStartOf="parent"
            />
        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/mainPageGuidLine2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginLeft="20dp"
            android:orientation="vertical"
            app:layout_constraintGuide_begin="349dp"
            app:layout_constraintStart_toStartOf="parent"
            />

        <TextView
            android:id="@+id/firstText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:fontFamily="@font/open_sans_light"
            android:text="One"
            android:textColor="@color/colorPrimary"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@+id/mainPageGuidLine"
            app:layout_constraintTop_toTopOf="parent"
            />
        <TextView
            android:id="@+id/secondText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/open_sans_extra_bold"
            android:text="View"
            android:textColor="@color/colorPrimary"
            android:textSize="24sp"
            app:layout_constraintStart_toEndOf="@+id/firstText"
            app:layout_constraintTop_toTopOf="@+id/firstText"
            android:layout_marginLeft="8dp"
            />
        <TextView
            android:id="@+id/thirdText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:fontFamily="@font/open_sans_light"
            android:text="Worth"
            android:textColor="@color/colorPrimary"
            android:textSize="24sp"
            app:layout_constraintStart_toEndOf="@+id/secondText"
            app:layout_constraintTop_toTopOf="@+id/secondText"
            android:layout_marginLeft="8dp"
            />
        <TextView
            android:id="@+id/fourthText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/open_sans_light"
            android:text="a"
            android:textColor="@color/colorPrimary"
            android:textSize="24sp"
            app:layout_constraintStart_toStartOf="@+id/mainPageGuidLine"
            app:layout_constraintTop_toBottomOf="@+id/firstText"
            />
        <TextView
            android:id="@+id/fifthText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginLeft="8dp"
            android:fontFamily="@font/open_sans_extra_bold"
            android:text="Thousands Word"
            android:textColor="@color/colorPrimary"
            android:textSize="24sp"
            app:layout_constraintStart_toEndOf="@+id/fourthText"
            app:layout_constraintTop_toTopOf = "@+id/fourthText"
            />
        <ImageView
            android:id="@+id/profileImageView"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:onClick="gotoMyProfile"
            app:layout_constraintEnd_toStartOf="@id/mainPageGuidLine2"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/ic_baseline_person_24"/>
        <SearchView
            android:id="@+id/mainPageSearchView"
            android:layout_width="0dp"
            android:layout_height="55dp"
            android:layout_marginTop="30dp"
            android:background="@drawable/custom_search_view"
            android:elevation="20dp"
            android:queryHint="Search here..."
            app:layout_constraintEnd_toStartOf="@id/mainPageGuidLine2"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="@id/mainPageGuidLine"
            app:layout_constraintTop_toBottomOf="@id/fourthText"
            tools:targetApi="lollipop"
            />
        <ScrollView
            android:id="@+id/mainPageScrollView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="45dp"
            android:scrollbars="none"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@id/mainPageGuidLine2"
            app:layout_constraintHorizontal_bias="0.506"
            app:layout_constraintStart_toStartOf="@id/mainPageGuidLine"
            app:layout_constraintTop_toBottomOf="@id/mainPageSearchView"
            app:layout_constraintVertical_bias="0.0">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.cardview.widget.CardView
                    android:id="@+id/housePageCardView"
                    android:layout_width="match_parent"
                    android:layout_height="226dp">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <View
                            android:id="@+id/housePageView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/image_one"
                            android:onClick="bringHouses"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="1.0"
                            />

                        <TextView
                            android:id="@+id/sixthText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="116dp"
                            android:fontFamily="@font/open_sans_extra_bold"
                            android:text="House Information"
                            android:textColor="@color/white"
                            android:textSize="22sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <RatingBar
                            android:id="@+id/housePageRatingBar"
                            style="?android:attr/ratingBarStyleSmall"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginTop="16dp"
                            android:max="5"
                            android:numStars="5"
                            android:progressTint="@color/colorAccent"
                            android:rating="4.5"
                            android:stepSize="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/sixthText" />
                        <TextView
                            android:id="@+id/seventhText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/open_sans_bold"
                            android:text="4.91"
                            android:textColor="@color/white"
                            app:layout_constraintStart_toEndOf="@+id/housePageRatingBar"
                            app:layout_constraintTop_toBottomOf="@+id/sixthText"

                            />
                        <TextView
                            android:id="@+id/eighthText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/open_sans_light"
                            android:text="(510)"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            app:layout_constraintStart_toEndOf="@+id/seventhText"
                            app:layout_constraintTop_toTopOf="@+id/seventhText"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/customerPageCardView"
                    android:layout_width="match_parent"
                    android:layout_height="226dp"
                    android:layout_marginTop="20dp">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <View
                            android:id="@+id/customerPageView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/image_two"
                            android:onClick="bringCustomers"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <TextView
                            android:id="@+id/ninthText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="116dp"
                            android:fontFamily="@font/open_sans_extra_bold"
                            android:text="Customer Information"
                            android:textColor="@color/white"
                            android:textSize="22sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <RatingBar
                            android:id="@+id/customerPageRatingBar"
                            style="?android:attr/ratingBarStyleSmall"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginTop="16dp"
                            android:max="5"
                            android:numStars="5"
                            android:progressTint="@color/colorAccent"
                            android:rating="4.5"
                            android:stepSize="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/ninthText" />
                        <TextView
                            android:id="@+id/tenthText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/open_sans_bold"
                            android:text="4.75"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            app:layout_constraintStart_toEndOf="@+id/customerPageRatingBar"
                            app:layout_constraintTop_toBottomOf="@+id/ninthText" />
                        <TextView
                            android:id="@+id/eleventhText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/open_sans_light"
                            android:text="(510)"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            app:layout_constraintStart_toEndOf="@+id/tenthText"
                            app:layout_constraintTop_toTopOf="@+id/tenthText"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/houseOwnerCardView"
                    android:layout_width="match_parent"
                    android:layout_height="226dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="15dp">
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">
                        <View
                            android:id="@+id/houseOwnerView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="@drawable/image_two"
                            android:onClick="bringHouseOwners"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <TextView
                            android:id="@+id/twelfthText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="116dp"
                            android:fontFamily="@font/open_sans_extra_bold"
                            android:text="House Owner Information"
                            android:textColor="@color/white"
                            android:textSize="22sp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.0"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <RatingBar
                            android:id="@+id/houseOwnerRatingBar"
                            style="?android:attr/ratingBarStyleSmall"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginLeft="12dp"
                            android:layout_marginTop="16dp"
                            android:max="5"
                            android:numStars="5"
                            android:progressTint="@color/colorAccent"
                            android:rating="4.5"
                            android:stepSize="0.5"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@id/twelfthText" />
                        <TextView
                            android:id="@+id/thirteenthText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginLeft="10dp"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/open_sans_bold"
                            android:text="4.75"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            app:layout_constraintStart_toEndOf="@+id/houseOwnerRatingBar"
                            app:layout_constraintTop_toBottomOf="@+id/twelfthText" />
                        <TextView
                            android:id="@+id/fourteenthText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="5dp"
                            android:layout_marginLeft="5dp"
                            android:fontFamily="@font/open_sans_light"
                            android:text="(510)"
                            android:textColor="@color/white"
                            android:textSize="15sp"
                            app:layout_constraintStart_toEndOf="@+id/thirteenthText"
                            app:layout_constraintTop_toTopOf="@+id/thirteenthText"/>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>
            </LinearLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>
</LinearLayout>

